	shorttitle = {The scope and precision of specific temporal expectancy},
	url = {http://link.springer.com/article/10.3758/s13414-010-0079-1},
	doi = {10.3758/s13414-010-0079-1},
	abstract = {Recent evidence from choice response time experiments with variable foreperiods ({FPs}) has shown that temporal expectancy can be event specific. When a certain target appears particularly frequent after one certain {FP}, participants tend to expect that target after that {FP}. This typically results in best performance for that target when it appears after that {FP}. In the present study, we investigated how temporally precise event-specific temporal expectancy is, and in which range of {FPs} it can be found. Two target stimuli were asymmetrically distributed over two “peak-{FPs}” and were equally distributed over 13 additional {FPs}. Event-specific expectancies were found for peak-{FP} pairs of 500/1,100 ms and 300/500 ms. Furthermore, the event expectancies generalized to a wide range of nonpeak {FPs} surrounding the peak {FPs}.},
	language = {en},
	number = {3},
	urldate = {2014-06-12},
	journal = {Attention, Perception, \& Psychophysics},
	author = {Thomaschke, Roland and Wagener, Annika and Kiesel, Andrea and Hoffmann, Joachim},
	month = apr,
	year = {2011},
	keywords = {Cognitive Psychology, Expectation, foreperiod, Preparation, Temporal cognition},
	pages = {953--964},
	file = {Full Text PDF:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/QUJ2R4BB/Thomaschke et al. - 2011 - The scope and precision of specific temporal expec.pdf:application/pdf;Snapshot:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/S3JHF744/10.html:text/html}
}

@article{tzagarakis_beta-band_2010,
	title = {Beta-Band Activity during Motor Planning Reflects Response Uncertainty},
	volume = {30},
	issn = {0270-6474, 1529-2401},
	url = {http://www.jneurosci.org/content/30/34/11270},
	doi = {10.1523/JNEUROSCI.6026-09.2010},
	abstract = {It has been known for many years that the power of beta-band oscillatory activity in motor-related brain regions decreases during the preparation and execution of voluntary movements. However, it is not clear yet whether the amplitude of this desynchronization is modulated by any parameter of the motor task. Here, we examined whether the degree of uncertainty about the upcoming movement direction modulated beta-band desynchronization during motor preparation. To this end, we recorded whole-head neuromagnetic signals while human subjects performed an instructed-delay reaching task with one, two, or three possible target directions. We found that the reduction of power of beta-band activity (16–28 Hz) during motor preparation was scaled relative to directional uncertainty. Furthermore, we show that the change of beta-band power correlates with the change of latency of response associated with response uncertainty. Finally, we show that the main source of beta-band desynchronization was located in the peri-Rolandic region. The results establish directional uncertainty as an important determinant of beta-band power during motor preparation and indicate that neural activity in the sensorimotor cortex during motor preparation covaries with directional uncertainty.},
	language = {en},
	number = {34},
	urldate = {2014-03-20},
	journal = {The Journal of Neuroscience},
	author = {Tzagarakis, Charidimos and Ince, Nuri F. and Leuthold, Arthur C. and Pellizzer, Giuseppe},
	month = aug,
	year = {2010},
	pmid = {20739547},
	pages = {11270--11277},
	file = {Full Text PDF:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/EI5A7F6E/Tzagarakis et al. - 2010 - Beta-Band Activity during Motor Planning Reflects .pdf:application/pdf;Full Text PDF:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/XE8RQF6B/Tzagarakis et al. - 2010 - Beta-Band Activity during Motor Planning Reflects .pdf:application/pdf;Snapshot:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/KA3ZJEJX/11270.html:text/html;Snapshot:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/XQUDHERU/11270.html:text/html}
