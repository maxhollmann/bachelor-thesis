	month = jun,
	year = {2001},
	keywords = {accuracy, decision making, hedderik, internal timekeeper, Models, Reaction Time, Response Latency, response speed, {RT}, serial choice {RT} tasks, Stimulus Intervals, Strategies, Time, timed decision enhancement model, timing strategies},
