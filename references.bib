@article{karmarkar_telling_2007,
	title = {Telling time in the absence of clocks.},
	volume = {53},
	issn = {0896-6273},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC1857310/},
	doi = {10.1016/j.neuron.2007.01.006},
	abstract = {Decisions based on the timing of sensory events are fundamental to sensory processing. However, the mechanisms by which the brain measures time over ranges of milliseconds to seconds remain unknown. The dominant model of temporal processing proposes that an oscillator emits events that are integrated to provide a linear metric of time. We examine an alternate model in which cortical networks are inherently able to tell time as a result of time-dependent changes in network state. Using computer simulations we show that within this framework, there is no linear metric of time, and that a given interval is encoded in the context of preceding events. Human psychophysical studies were used to examine the predictions of the model. Our results provide theoretical and experimental evidence that for short intervals there is no linear metric of time, and that time may be encoded in the high-dimensional state of local neural networks.},
	number = {3},
	urldate = {2014-07-08},
	journal = {Neuron},
	author = {Karmarkar, Uma R. and Buonomano, Dean V.},
	month = feb,
	year = {2007},
	pmid = {17270738},
	pmcid = {PMC1857310},
	pages = {427--438}
}

