	keywords = {Accumulation, Auditory evoked potentions, Bisection, {CNV}, Interval timing, Response caution, {TMS}},
	pages = {27--28},
	file = {ScienceDirect Full Text PDF:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/GBHWH6PU/van Rijn et al. - 2014 - The Role of the SMA and the Contingent Negative Va.pdf:application/pdf;ScienceDirect Snapshot:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/KM45X9Z8/S1877042814018539.html:text/html}
