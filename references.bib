}

@article{fischer_temporo-spatial_2010,
	title = {Temporo-Spatial Dynamics of Event-Related {EEG} Beta Activity during the Initial Contingent Negative Variation},
	volume = {5},
	url = {http://dx.doi.org/10.1371/journal.pone.0012514},
	doi = {10.1371/journal.pone.0012514},
	abstract = {In the electroencephalogram ({EEG}), early anticipatory processes are accompanied by a slow negative potential, the initial contingent negative variation ({iCNV}), occurring between 500 and 1500 ms after cue onset over prefrontal cortical regions in tasks with cue-target intervals of about 3 s or longer. However, the temporal sequence of the distributed cortical activity contributing to {iCNV} generation remains unclear. During {iCNV} generation, selectively enhanced low-beta activity has been reported. Here we studied the temporal order of activation foci in cortical regions assumed to underlie {iCNV} generation using source reconstruction of low-beta (13–18 Hz) activity. During the {iCNV}, elicited by a cued simple reaction-time task, low-beta power peaked first (750 ms after cue onset) in anterior frontal and limbic regions and last (140 ms later) in posterior areas. This activity occurred 3300 ms before target onset and provides evidence for the temporally ordered involvement of both cognitive-control and motor-preparation processes already at early stages during the preparation for speeded action.},
	number = {9},
	urldate = {2014-06-15},
	journal = {{PLoS} {ONE}},
	author = {Fischer, Thomas and Langner, Robert and Diers, Kersten and Brocke, Burkhard and Birbaumer, Niels},
	month = sep,
	year = {2010},
	pages = {e12514},
	file = {PLoS Full Text PDF:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/JJTCM7QG/Fischer et al. - 2010 - Temporo-Spatial Dynamics of Event-Related EEG Beta.pdf:application/pdf;PLoS Snapshot:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/KJSB3ZQF/infodoi10.1371journal.pone.html:text/html}
}

@article{fogelson_different_2006,
	title = {Different Functional Loops between Cerebral Cortex and the Subthalmic Area in Parkinson's Disease},
	volume = {16},
	issn = {1047-3211, 1460-2199},
	url = {http://cercor.oxfordjournals.org/content/16/1/64},
	doi = {10.1093/cercor/bhi084},
	abstract = {We investigate the extent to which functional circuits coupling cortical and subthalamic activity are multiple and segregated by frequency in untreated Parkinson's disease ({PD}). To this end, we recorded {EEG} and local field potentials ({LFPs}) from macroelectrodes inserted into the subthalamic nucleus area ({SA}) in nine awake patients following functional neurosurgery for {PD}. Patients were studied after overnight withdrawal of medication. Coherence between {EEG} and {SA} {LFPs} was apparent in the theta (3–7 Hz), alpha (8–13 Hz), lower beta (14–20 Hz) and upper beta (21–32 Hz) bands, although activity in the alpha and upper beta bands dominated. Theta coherence predominantly involved mesial and lateral areas, alpha and lower beta coherence the mesial and ipsilateral motor areas, and upper beta coherence the midline cortex. {SA} {LFPs} led {EEG} in the theta band. In contrast, {EEG} led the depth {LFP} in the lower and upper beta bands. {SA} {LFP} activity in the alpha band could either lead or lag {EEG}. Thus there are several functional sub-loops between the subthalamic area and cerebral cortical motor regions, distinguished by their frequency, cortical topography and temporal relationships. Tuning to distinct frequencies may provide a means of marking and segregating related processing, over and above any anatomical segregation of processing streams.},
	language = {en},
	number = {1},
	urldate = {2014-03-28},
	journal = {Cerebral Cortex},
	author = {Fogelson, Noa and Williams, David and Tijssen, Marina and Bruggen, Gerard van and Speelman, Hans and Brown, Peter},
	month = jan,
	year = {2006},
	pmid = {15829734},
	keywords = {{EEG}, oscillations, Parkinson's disease, subthalamic nucleus, synchronization},
	pages = {64--75},
	file = {Full Text PDF:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/IBAVK6WR/Fogelson et al. - 2006 - Different Functional Loops between Cerebral Cortex.pdf:application/pdf;Snapshot:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/UE8EJM46/64.html:text/html}
}

@article{forstmann_striatum_2008,
	title = {Striatum and pre-{SMA} facilitate decision-making under time pressure},
	volume = {105},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/105/45/17538},
	doi = {10.1073/pnas.0805903105},
	abstract = {Human decision-making almost always takes place under time pressure. When people are engaged in activities such as shopping, driving, or playing chess, they have to continually balance the demands for fast decisions against the demands for accurate decisions. In the cognitive sciences, this balance is thought to be modulated by a response threshold, the neural substrate of which is currently subject to speculation. In a speed decision-making experiment, we presented participants with cues that indicated different requirements for response speed. Application of a mathematical model for the behavioral data confirmed that cueing for speed lowered the response threshold. Functional neuroimaging showed that cueing for speed activates the striatum and the pre-supplementary motor area (pre-{SMA}), brain structures that are part of a closed-loop motor circuit involved in the preparation of voluntary action plans. Moreover, activation in the striatum is known to release the motor system from global inhibition, thereby facilitating faster but possibly premature actions. Finally, the data show that individual variation in the activation of striatum and pre-{SMA} is selectively associated with individual variation in the amplitude of the adjustments in the response threshold estimated by the mathematical model. These results demonstrate that when people have to make decisions under time pressure their striatum and pre-{SMA} show increased levels of activation.},
	language = {en},
	number = {45},
	urldate = {2014-06-16},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Forstmann, Birte U. and Dutilh, Gilles and Brown, Scott and Neumann, Jane and Cramon, D. Yves von and Ridderinkhof, K. Richard and Wagenmakers, Eric-Jan},
	month = nov,
	year = {2008},
	pmid = {18981414},
	keywords = {Basal ganglia, {fMRI}, linear ballistic accumulator model, speed-accuracy tradeoff},
	pages = {17538--17542},
	file = {Full Text PDF:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/6JGQVVHS/Forstmann et al. - 2008 - Striatum and pre-SMA facilitate decision-making un.pdf:application/pdf;Snapshot:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/FPGUU2T2/17538.html:text/html}
}

@phdthesis{fowler_time_2000,
	address = {{US}},
	title = {Time of day effects on cognitive processing and brain temperature},
	abstract = {Cognitive performance appears to vary as a function of the time of day in which the task is performed. This fluctuation in performance often is related to the circadian rhythm of body temperature. Fluctuations in cognitive performance have also been linked to changes in cerebral blood flow (as measured by brain temperature). This study was designed to determine whether task characteristics are differentially sensitive to time of day effects, resulting in differences in cognitive performance. Also, this study sought to determine if there is a time of day effect on the way in which brain temperature changes as a consequence of task performance. Tympanic membrane ({TM}) thermometry was used to measures changes in {TM} temperature while subjects performed a series of 4 cognitive tasks. {TM} temperature reflects average brain temperature and is an indirect measure of cerebral blood flow ({CBF}). Results showed that, as in previous studies, there is a time of day effect on body temperature. Only measures of short-term memory for word lists demonstrated a significant time of day effect. Results of this study indicate that {CBF} changes as a result of performing specific tasks. These changes in {CBF} are different at different times for the some tasks, even if there is no significant difference in performance. A significant interaction exists between the time of day in which a task is performed, the change in {CBF}, and the way in which the {CBF} changes. Implications for future time of day studies are discussed. ({PsycINFO} Database Record (c) 2012 {APA}, all rights reserved)},
	school = {{ProQuest} Information \& Learning},
	author = {Fowler, Laurie A.},
	month = sep,
	year = {2000},
	keywords = {Body temperature, Brain, Cognitive Processes, Time, time of day of performance, cognitive processing \& brain temperature, human Ss}
}

