	pages = {1798--1806},
	file = {ScienceDirect Full Text PDF:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/GB3UJSSB/Martin et al. - 2008 - Interval timers and coupled oscillators both media.pdf:application/pdf;ScienceDirect Snapshot:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/DSJ7Z7NJ/S1053811908000839.html:text/html}
}

