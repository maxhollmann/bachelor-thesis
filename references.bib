	pages = {735--746},
	file = {Full Text PDF:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/BFBVS9H7/Kühn et al. - 2004 - Event‐related beta desynchronization in human subt.pdf:application/pdf;Full Text PDF:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/CTWQ2NM6/Kühn et al. - 2004 - Event‐related beta desynchronization in human subt.pdf:application/pdf;Snapshot:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/48649UD6/735.html:text/html;Snapshot:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/IGG8PEJG/735.html:text/html}
