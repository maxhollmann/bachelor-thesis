	issn = {0091-3057},
	shorttitle = {Differential effects of cocaine and ketamine on time estimation},
	url = {http://www.sciencedirect.com/science/article/pii/S0091305706002395},
	doi = {10.1016/j.pbb.2006.07.019},
	abstract = {The present experiment examined the effects of cocaine (0.0 and 15 mg/kg, i.p.) and ketamine (0.0, 10.0 and 15 mg/kg, i.p.) on timing behavior using a 12-s differential reinforcement of low rates ({DRL}) procedure and a 2- vs. 8-s bisection procedure in rats. {DRL} (time production) and bisection (time perception) procedures are sensitive to effects of dopaminergic drugs and provide an assessment of the accuracy and precision of interval timing as well as the subject's level of impulsivity. When administered to rats trained on either the {DRL} or the bisection procedure, cocaine shifted the psychophysical functions leftward relative to control conditions. In contrast, ketamine produced no change in the temporal control of behavior on either procedure. These differential effects of cocaine and ketamine are consistent with previous reports suggesting that dopamine levels in the dorsal striatum, but not in prefrontal cortex, ventral striatum or hippocampal regions, are crucial for the regulation of the speed of an internal clock.},
	number = {1},
	urldate = {2014-03-04},
	journal = {Pharmacology Biochemistry and Behavior},
	author = {Cheng, Ruey-Kuang and MacDonald, Christopher J. and Meck, Warren H.},
	month = sep,
	year = {2006},
	keywords = {Cortex, Dopamine, Glutamate, {NMDA}, Striatum, Time perception, Timing},
	pages = {114--122},
	file = {ScienceDirect Full Text PDF:/home/max/.mozilla/firefox/cq0juvrm.default/zotero/storage/IVRGFZSX/Cheng et al. - 2006 - Differential effects of cocaine and ketamine on ti.pdf:application/pdf}
}

